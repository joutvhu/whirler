{
    "name": "@whirler/server",
    "version": "0.0.4",
    "description": "A library support development single endpoint api.",
    "author": "Jout V Hu (Giao Ho)",
    "license": "MIT",
    "main": "lib/index.js",
    "module": "lib/index.mjs",
    "directories": {
        "lib": "./lib"
    },
    "files": [
        "lib",
        "README.md",
        "LICENSE"
    ],
    "publishConfig": {
        "access": "public"
    },
    "homepage": "https://github.com/joutvhu/whirler",
    "bugs": {
        "url": "https://github.com/joutvhu/whirler/issues"
    },
    "repository": {
        "type": "git",
        "url": "http://github.com/joutvhu/whirler.git"
    },
    "keywords": [
        "whirler",
        "whirler server",
        "whirler js",
        "single endpoint",
        "api",
        "method"
    ],
    "engines": {
        "node": ">= 8.x"
    },
    "scripts": {
        "build": "babel --config-file ./config/.babelrc ./lib/ --out-dir ./lib/",
        "lint": "eslint --config ./config/.eslintrc --ext=mjs ./lib/",
        "lint:js": "eslint --config ./config/.eslintrc --ext=js ./lib/",
        "test": "jest --verbose --config=config/jest.config.js"
    },
    "devDependencies": {
        "@babel/cli": "^7.4.3",
        "@babel/core": "^7.4.3",
        "@babel/plugin-transform-modules-commonjs": "^7.4.3",
        "eslint": "^5.16.0",
        "jest": "^24.7.0"
    }
